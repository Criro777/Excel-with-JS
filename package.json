{
  "name": "excel-js-project",
  "version": "1.0.0",
  "description": "Pure JavaScript Excel Application",
  "main": "webpack.config.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "cross-env NODE_ENV=development webpack-dev-server --open",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "lint": "npx eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Criro777/JSExcel.git"
  },
  "keywords": [
    "js",
    "excel",
    "es6"
  ],
  "author": "Vyacheslav <vyacheslav@mail.ru>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Criro777/JSExcel/issues"
  },
  "homepage": "https://github.com/Criro777/JSExcel#readme",
  "browserslist": "> 0.25%, not dead",
  "private": false,
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/assetsTransformer.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.8.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-jest": "^24.1.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "normalize.css": "^8.0.1"
  }
}
